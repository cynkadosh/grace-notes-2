{
  "locals": {
    "url": "http://localhost:8080",
    "name": "Grace Notes",
    "owner": "Grace Notes",
    "description": "Training in Bible Teaching"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-sass",
    "./node_modules/wintersmith-coffee",
    "./plugins/pandoc.coffee",
    "./plugins/topics.coffee",
    "./plugins/epub.coffee",
    "./plugins/pdf.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3,
    "first": "blog/index.html",
    "filename": "blog/%d/index.html"
  }
}
